const myAge = 33; // This is the users age

let earlyYears = 2; // This may change

earlyYears *= 10.5;

let laterYears = myAge - 2;

laterYears *= 4; // Dog years

let myAgeInDogYears = earlyYears + laterYears; // This is my age in dog years

let myName = 'Lauren'.toLowerCase(); // My name in lower case

console.log(`My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`)
// Blah